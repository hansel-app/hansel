{
  "name": "hansel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preserve": "npm run proto",
    "serve": "vue-cli-service serve",
    "prebuild": "npm run proto",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "proto": "rm -rf ./src/protobuf && mkdir ./src/protobuf && protoc --proto_path ../api-specs --js_out=import_style=commonjs,binary:./src/protobuf --grpc-web_out=import_style=commonjs+dts,mode=grpcwebtext:./src/protobuf $(find ../api-specs -type f -name \"*.proto\")"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "core-js": "^3.6.5",
    "dayjs": "^1.10.7",
    "google-protobuf": "^3.18.0",
    "grpc-web": "^1.2.1",
    "js-cookie": "^3.0.1",
    "protoc-gen-grpc-web": "^1.2.1",
    "register-service-worker": "^1.7.1",
    "swiper": "^6.8.4",
    "vant": "^3.2.2",
    "vue": "^3.0.0",
    "vue-axios": "^3.3.7",
    "vue-class-component": "^8.0.0-0",
    "vue-gtag": "^2.0.1",
    "vue-router": "^4.0.11",
    "vue3-click-away": "^1.2.1",
    "vue3-google-map": "^0.8.2",
    "vue3-touch-events": "^4.1.0",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.5",
    "@types/google.maps": "^3.45.6",
    "@types/js-cookie": "^2.2.7",
    "@types/swiper": "^5.4.3",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "less": "^4.1.1",
    "less-loader": "^7.3.0",
    "ts-import-plugin": "^1.6.7",
    "typescript": "~4.1.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "globals": {
      "google": "readonly"
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "ignorePatterns": [
      "src/protobuf/*"
    ],
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
